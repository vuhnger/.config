
[env]
TERM = "xterm-256color"

[window]
decorations = "buttonless"
dynamic_padding = false
opacity = 1
blur = false

  [window.padding]
  x = 25
  y = 20

  [window.dimensions]
  columns = 133
  lines = 40

[bell]
animation = "Linear"
duration = 0
color = "#b59b9c"

[font]
size = 15

  [font.offset]
  x = 1
  y = 1

  [font.bold]
  family = "SFMono Nerd Font"
  style = "Heavy"

  [font.bold_italic]
  family = "SFMono Nerd Font"
  style = "Heavy Italic"

  [font.italic]
  family = "SFMono Nerd Font"
  style = "Medium Italic"

  [font.normal]
  family = "SFMono Nerd Font"
  style = "Medium"

[[keyboard.bindings]]
key = "Right"
mods = "Alt"
chars = "\u001bF"

[[keyboard.bindings]]
key = "Left"
mods = "Alt"
chars = "\u001bB"

[general]
live_config_reload = true
import = [
  "/opt/homebrew/lib/node_modules/alacritty-themes/themes/Catppuccin-Macchiato.toml"
]
